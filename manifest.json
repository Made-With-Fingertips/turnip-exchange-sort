{
    "name": "Turnip Exchange Sort",
    "version": "1.1.0",
    "description": "Sort islands on turnip.exchange",
    "manifest_version": 2,
    "permissions": [
        "activeTab"
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "images/turnip32.png",
        "default_title": "Turnip Exchange Sort"
    },
    "icons": {
        "16": "images/turnip16.png",
        "32": "images/turnip32.png",
        "48": "images/turnip48.png",
        "128": "images/turnip128.png"
    }
}